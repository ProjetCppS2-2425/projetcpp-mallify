  <property name="styleSheet">
   <string notr="true">/* ====== Background ====== */ #background { background: #e9ecef; } 
/* ====== Sidebar ====== */ #sidebar { background-color: #2C3E50; } 
/* ====== Containers ====== */ #function_frame, #information_emp, #abs_back { background-color: #f8f9fa; }
/* ====== Sidebar Buttons ====== */ #modules_layout QPushButton { text-align: left; color: #ECF0F1; border: none; background: transparent; font-size: 13px; padding: 0px; border-radius: 4px; display: flex; align-items: center; transition: background 0.2s ease-in-out, box-shadow 0.2s ease-in-out; }  
#modules_layout QPushButton:hover { background-color: rgba(255, 255, 255, 0.1); color: #FFFFFF; }  
#modules_layout QPushButton:pressed { background-color: rgba(255, 255, 255, 0.2); }  
/* ====== Profile Info ====== */ #nom { background-color: rgba(0, 0, 0, 0.2); color: #ffffff; border-radius: 5px; font-size: 13px; font-weight: bold; text-align: center; padding: 0px; }  
#role { color: #BDC3C7; font-size: 10px; text-align: center; padding: 0px; }  
/* ====== Settings &amp; Logout ====== */ #logout, #settings { background: none; border: none; color: #ECF0F1; font-size: 12px; transition: color 0.2s; padding: 0px; }  
#logout:hover { color: #E74C3C; }  
#settings:hover { color: #3498DB; }
/* ====== CONTAINER ====== */ 
#function_frame { background-color: #f8f9fa; border-radius: 5px; border: 1px solid #dcdde1; }
/* ====== FILTER &amp; SEARCH ELEMENTS ====== */ 
#city_filter_emp, #role_filter_emp, #date1_emp, #date2_emp, #searchbox_emp { border: 1px solid #bdc3c7; border-radius: 4px; background-color: #ffffff; transition: border 0.3s ease-in-out, box-shadow 0.2s; padding: 0px; } 
#city_filter_emp:focus, #role_filter_emp:focus, #date1_emp:focus, #date2_emp:focus, #searchbox_emp:focus { border-color: #3498DB; box-shadow: 0px 0px 4px rgba(52, 152, 219, 0.5); }
/* ====== SEARCH FIELD &amp; ICON ====== */ 
#searchbutton { background-color: blue; border: none; cursor: pointer; transition: color 0.3s ease-in-out;border-radius: 4px; } 
#searchbutton:hover { background-color: #3498DB; color: white; }
#reset, #searchbutton { background-color: transparent; border: 1px solid #bdc3c7; border-radius: 4px; min-width: 28px; height: 28px; cursor: pointer; transition: background 0.3s, border 0.3s; }
#reset:hover, #searchbutton:hover { background-color: #3498DB; border-color: #2980B9; }
#reset:pressed, #searchbutton:pressed { background-color: #1F618D; border-color: #154360; }
#reset_button_2, #export_button_2, #delete_button_2 { font-size: 12px; font-weight: bold; border: none; text-align: center; transition: background 0.2s ease-in-out, box-shadow 0.2s ease-in-out; }
#reset_button_2 { background-color: #BDC3C7; color: black; } #reset_button_2:hover { background-color: #AAB7B8; box-shadow: 0px 0px 5px rgba(189, 195, 199, 0.5); }
#export_button_2 { background-color: #3498DB; color: white; } #export_button_2:hover { background-color: #2980B9; box-shadow: 0px 0px 5px rgba(52, 152, 219, 0.5); }
#delete_button_2 { background-color: #E74C3C; color: white; } #delete_button_2:hover { background-color: #C0392B; box-shadow: 0px 0px 5px rgba(231, 76, 60, 0.5); }
/* ====== INFORMATION EMP STYLING ====== */
#information_emp { background-color: #ffffff; border-radius: 6px; border: 1px solid #dcdde1; }
#information_emp QLabel { color: #2C3E50; font-weight: bold; font-size: 12px; }
#information_emp QLineEdit, #information_emp QComboBox, #information_emp QDateEdit {
    background-color: #f5f5f5; border: 1px solid #bdc3c7; border-radius: 4px; font-size: 10px; color: #333;
}
#information_emp QLineEdit:focus, #information_emp QComboBox:focus, #information_emp QDateEdit:focus {
    border-color: #3498DB; box-shadow: 0px 0px 5px rgba(52, 152, 219, 0.5);
}
#information_emp QCheckBox { color: #2C3E50; font-weight: bold; }
#information_emp QCheckBox::indicator { width: 16px; height: 16px; background: #ffffff; border: 1px solid #bdc3c7; }
#information_emp QCheckBox::indicator:checked { background: #3498DB; border: 1px solid #1F618D; }
/* BUTTON STYLING */
#update_button, #add_button, #reset_button, #delete_button {
    border: none;
    font-size: 12px;
    font-weight: bold;
    color: white;
    transition: background 0.2s ease-in-out, box-shadow 0.2s ease-in-out;
}
#update_button, #add_button, #reset_button, #delete_button, #export_button { border: none; font-size: 12px; font-weight: bold; color: white; transition: background 0.2s ease-in-out, box-shadow 0.2s ease-in-out; }
#update_button { background-color: #F39C12; } #update_button:hover { background-color: #D68910; box-shadow: 0px 0px 5px rgba(243, 156, 18, 0.5); }
#add_button { background-color: #27AE60; } #add_button:hover { background-color: #229954; box-shadow: 0px 0px 5px rgba(39, 174, 96, 0.5); }
#reset_button { background-color: #BDC3C7; color: black; } #reset_button:hover { background-color: #95A5A6; color: black; box-shadow: 0px 0px 5px rgba(189, 195, 199, 0.5); }
#delete_button { background-color: #E74C3C; } #delete_button:hover { background-color: #C0392B; box-shadow: 0px 0px 5px rgba(231, 76, 60, 0.5); }
#export_button { background-color: #3498DB; } #export_button:hover { background-color: #2980B9; box-shadow: 0px 0px 5px rgba(52, 152, 219, 0.5); }
/* ====== SCROLLBAR ====== */
QScrollBar:vertical { border: none; background: #ECF0F1; width: 8px; }
QScrollBar::handle:vertical { background: #BDC3C7; border-radius: 4px; }
QScrollBar::handle:vertical:hover { background: #AAB7B8; }
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical { background: none; height: 0px; }
/* ====== CONTAINER STYLING ====== */
#abs_back { background-color: #f8f9fa; border-radius: 5px; border: 1px solid #dcdde1; }
/* ====== TABLE &amp; HEADERS ====== */
#table_abs,#table_emp { border-radius: 5px; background-color: #ffffff; border: 1px solid #dcdde1; }
QHeaderView::section { background-color: #2C3E50; color: white; border: none; text-align: left; padding: 4px; }
QTableWidget::item { border-bottom: 1px solid #d0d3d4; color: #2C3E50; padding-left: 5px; }
QTableWidget::item:selected { background-color: rgba(46, 134, 222, 0.2); color: #2C3E50; }
/* ===== INPUTS &amp; FILTERS ====== */
#date_abs, #filter_abs { border: 1px solid #bdc3c7; border-radius: 3px; background: white; font-size: 12px; }
#date_abs:focus, #filter_abs:focus { border-color: #3498DB; box-shadow: 0px 0px 4px rgba(52, 152, 219, 0.5); }
#reset_abs, #export_abs { font-size: 12px; font-weight: bold; border: none; text-align: center; transition: background 0.2s ease-in-out, box-shadow 0.2s ease-in-out; }
#reset_abs { background-color: #BDC3C7; color: black; } #reset_abs:hover { background-color: #AAB7B8; box-shadow: 0px 0px 5px rgba(189, 195, 199, 0.5); }
#export_abs { background-color: #3498DB; color: white; } #export_abs:hover { background-color: #2980B9; box-shadow: 0px 0px 5px rgba(52, 152, 219, 0.5); }
/* ====== TOOLBUTTONS ====== */
#reset { background: transparent; border: none; color:black; }
#reset:hover { color: gray; }
/* ====== INPUT TEXT COLOR FIX ====== */
QLineEdit, QComboBox, QDateEdit { color: black; }
QLineEdit::placeholder, QComboBox::placeholder, QDateEdit::placeholder { color: rgba(0, 0, 0, 0.3); }
#title{color:black;}

</string>
