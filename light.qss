/* LOGIN PAGE */
QMainWindow { background: #F0EDE8; }
#login_page { background: #FFFFFF; border-radius: 15px; padding: 12px; border: 2px solid #D32F2F; }
#logo_label { background: transparent; border: none; margin: 8px auto; qproperty-alignment: AlignCenter; }
#username, #password { background: transparent; border: none; border-bottom: 2px solid #D32F2F; color: #3E3E3E; padding: 4px; font-size: 12px; }
#username:focus, #password:focus { border-bottom: 3px solid #B71C1C; }
#username::placeholder, #password::placeholder { color: rgba(62, 62, 62, 0.8); }
#login_btn, #login_btn_2 { border-radius: 8px; padding: 8px; font-weight: bold; font-size: 12px; }
#login_btn { background: qlineargradient(x1:0, y1:0.505, x2:1, y2:0.477, stop:0 #FF5C5C, stop:1 #D32F2F); color: white; border: 2px solid #B71C1C; }
#login_btn:hover { background: qlineargradient(x1:0, y1:0.505, x2:1, y2:0.477, stop:0 #E55353, stop:1 #A32020); }
#login_btn:pressed { background: #A32020; }
#login_btn_2 { background: qlineargradient(x1:0, y1:0.505, x2:1, y2:0.477, stop:0 #C0C0C0, stop:1 #A0A0A0); color: white; border: 2px solid #8A8A8A; }
#login_btn_2:hover { background: qlineargradient(x1:0, y1:0.505, x2:1, y2:0.477, stop:0 #A0A0A0, stop:1 #7A7A7A); }
#login_btn_2:pressed { background: #7A7A7A; }
#forgot_password_btn { background: none; border: none; color: #D32F2F; font-size: 10px; font-weight: bold; text-decoration: underline; }
#forgot_password_btn:hover { color: #B71C1C; }
/* SIDEBAR & TOPBAR */
#sidebar { min-width: 250px; max-width: 250px; background: #F5F5F5; border-right: 2px solid #FF5C5C; }
#sidebar QLabel { margin-top: 30px; margin-left: 80px; background: transparent; qproperty-pixmap: url("C:/Users/aab62/Desktop/testing/aab6/icons/logo_sidebar_light.png"); }
#sidebar QPushButton { background: #E55353; font-size: 14px; border: 2px solid transparent; border-radius: 8px; padding: 10px; color: white; }
#sidebar QPushButton:hover { background: #FF5C5C; border: 2px solid #E55353; }
#sidebar QPushButton:checked, #sidebar QPushButton:focus { background: #D32F2F; color: white; font-weight: bold; border: 2px solid #B71C1C; border-top-width: 2px; border-top-style: solid; border-top-color: rgba(255, 92, 92, 0.9);border-left-width: 2px; border-left-style: solid; border-left-color: rgba(255, 92, 92, 0.6); }
#topbar { min-height: 64px; max-height: 64px; background: #F5F5F5; border-bottom: 2px solid #FF5C5C; padding: 0 10px; }
#topbar QLabel { color:#3E3E3E; background: transparent; font-size: 18px; font-weight: bold; qproperty-alignment: AlignCenter; padding: 6px 12px; }
#topbar QToolButton { background: transparent; border: none; padding: 6px; }
#topbar #sidebar_btn {qproperty-icon: url("C:/Users/aab62/Desktop/testing/aab6/icons/logo_sidebar_light.png");}
#topbar QToolButton:hover { background: rgba(255, 92, 92, 0.3); border-radius: 4px; }
/* PROFILE SECTION */
#profile_section { background: #FFFFFF; border: 2px solid #FF5C5C; border-radius: 8px; padding: 10px; }
#profile_section QLabel { font-size: 14px; font-weight: bold; }
#profile_section QPushButton { background: #FF5C5C; border: none; border-radius: 6px; padding: 8px; color: white; }
#profile_section QPushButton:checked, #profile_section QPushButton:focus { background: #E55353; }
#profile_section QPushButton:hover { background: #D32F2F; }